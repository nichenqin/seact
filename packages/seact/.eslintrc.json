{
  "extends": "airbnb-base",
  "rules": {
    "semi": ["error", "never"],
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 0,
    "no-use-before-define": ["error", { "functions": false }]
  },
  "globals": {
    "MSForeignSymbol": true,
    "MSUserAssetLibrary": true,
    "NSURL": true
  }
}
